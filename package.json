  },
  "bugs": {
    "url": "https://github.com/stephenreynolds/kuminet/issues"
  },
  "main": "index.js",
  "//": "If you add or change the names of destinations in screeps.json, make sure you update these scripts to reflect the changes",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "build": "rollup -c",
    "push-main": "rollup -c --environment DEST:main",
    "push-sim": "rollup -c --environment DEST:sim",
    "push-dev": "rollup -c --environment DEST:dev",
    "push-private": "rollup -c --environment DEST:private",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-sim": "rollup -cw --environment DEST:sim",
    "gen-docs": "typedoc --out docs src"
  },
  "devDependencies": {
    "@types/lodash": "^3.10.1",
    "@types/node": "^10.5.5",
    "@types/screeps": "^2.4.0",
    "@types/screeps-profiler": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@typescript-eslint/typescript-estree": "^3.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "git-rev-sync": "^1.12.0",
    "lodash": "^4.17.11",
    "prettier": "^1.14.0",
    "rollup": "^0.63.4",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-nodent": "^0.2.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-screeps": "^0.1.2",
    "rollup-plugin-typescript2": "^0.16.1",
    "screeps-profiler": "^2.0.1",
    "ts-node": "^7.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "source-map": "~0.6.1"
  }
